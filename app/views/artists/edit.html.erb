<h1> Edit an Artist </h1>

<%= form_for @artist do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :age %>
<%= f.number_field :age %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :instrument %>
<%= f.collection_select :instruments, Instrument.all, :id, :name %>
<%= f.submit %>
<% end %>